#include <iostream>
using namespace std;

int main()
{
    char name[30][20] ;
    
    for ( int i = 0; i < 30; i++ )
    {
        cout << "Enter the students name: ";
        cin.getline( name[i], 20 );
    }
    
    for ( int i = 0; i < 30; i++ )
    {
        cout << name[i] << endl;
    }
}

